{
  "name": "express-ts",
  "version": "1.0.1",
  "description": "Express with TypeScript",
  "main": "main.js",
  "scripts": {
    "test": "jest",
    "build": "tsc --build",
    "start": "node --env-file=.env ./dist/main.js",
    "dev": "nodemon --exec npx tsx --env-file=.env ./src/main.ts",
    "prettier": "npx prettier . --write",
    "prisma": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio"
  },
  "keywords": [
    "typescript",
    "express",
    "prisma",
    "api",
    "rest"
  ],
  "author": "Villy15",
  "license": "ISC",
  "prisma": {
    "schema": "./src/prisma/schema.prisma",
    "seed": "ts-node ./src/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.11",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "pino": "^9.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/pino": "^7.0.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "nodemon": "^3.1.4",
    "pino-pretty": "^11.2.1",
    "prettier": "3.3.2",
    "prisma": "^5.15.1",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  }
}
